--objects (+ collision)
function make_ball(x,y,t)
	return {
		t=t,--type
		x=x, y=y,
		dx=0,dy=0,
		dir=v(0,0),
		spd=3,
		
		bx=1,by=1,
		bw=6,bh=6,
		
		spr=t,
		ghosttimer=0,
		kickable = true,
		activate = true,
		cd = 0,
		cdmax = 4,
	}
end

function update_objs()
	for o in all(objects)do
	o.cd = max(o.cd-1,0)
		o.x += o.dx
		o.y += o.dy
		local x=o.x
		local y=o.y
		
		o.kicked = not(kickable)
		
		o.ghosttimer -= 1
		
		o.debug=obj_coll(o,p)
		if o.kickable and obj_coll(o,p) then
			--determine kick axis
			local axis=v(2,0)
			if(abs(p.dy)>abs(p.dx))axis=v(0,2)
			
			o.kicked = true
			o.dx = sgn(p.dx)*axis.x
			o.dy = sgn(p.dy)*axis.y
			
			--dmg player if spiked
			if(o.t==6) edit_mode()
		end
		collide(o,1)
		
		if o.kicked then
			--shell/ball collision
			for i in all(objects)do
				if obj_coll(o,i) and 
				(i.x!=o.x or i.y!=o.y)then
					del(objects,o) 
					del(objects,i)
					burst_ptc(o.x+4,o.y+4,15)
				end
			end
			
		end
		
		if x%8==0 and y%8==0 then 
			map_x = x/8
			map_y = y/8
			check = true
		else 
			check = false
		end
		if check then
			for i = -1,1,2 do
				if mget(map_x+i,map_y)==3 then
					
					o.dx = -i*2
					
					o.dy = 0
				end
			end
			for i = -1,1,2 do
				if mget(map_x,map_y+i)==4 then
					
					o.dx = 0
					o.dy = -i*2
				end
			end
			
		end
	end
end

