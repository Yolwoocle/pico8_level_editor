--player

function init_player()
	p={
	 x=xstart,y=ystart,
	 dx=0,dy=0,
	 
	 bx=2,by=2,
	 bw=6,bh=6,
	 
	 g=0.3,
	 
	 spd=.4,
	 fric=.75,
	 
	 spr=64,
	 dashcd = 0,
	 activate = false,
	 cd = 0,
	 cdmax = 10,
	}
	--blockselected=block_list[1]
	--nb_blockselected=0
end

function update_player()
	p.cd = max(p.cd-1,0)
	p.dashcd = max(p.dashcd-1,0)
	p.activate = false
	
	--dashing
	if (p.dashcd > 11) p.activate = true
	prevo = btno
	btno = btn(üÖæÔ∏è)
	spd=p.spd
	if p.dashcd==0 and btno 
	and not(prevo) then 
		spd = 4 
		p.dashcd = 20
		scrshake=1
		
	end
	if p.dashcd > 11 then
		make_ptc(p.x+4,p.y+4,7)
	end
	
	
	if (abtn(‚¨ÖÔ∏è)) p.dx-=spd
	if (abtn(‚û°Ô∏è)) p.dx+=spd
	if (abtn(‚¨ÜÔ∏è)) p.dy-=spd
	if (abtn(‚¨áÔ∏è)) p.dy+=spd
	
	p.dx *= p.fric
	p.dy *= p.fric
	
	collide(p)
	
	p.x += p.dx
	p.y += p.dy
end


