-- particles, scrshake & polish
function init_ptc()
	particles = {}
end

function make_ptc(x,y,col)
	add(particles,{
		type="r",
		x=x,y=y,
		dx=rrnd(1),dy=rrnd(1),
		col=col,
		r=1+rnd(5)
	})
end

function burst_ptc(x,y,col)
	for i=1,15 do
		make_ptc(x,y,col)
	end
end

function make_textptc(x,y,txt)
	add(particles,{
		type="t",
		x=x,y=y,
		dx=0,dy=-1,
		r=300,
		txt=txt,
	})
end

function update_ptc()
	for p in all(particles)do
		p.dx *= 0.95
		p.dy *= 0.95
		p.r *= 0.95
		
		p.x += p.dx
		p.y += p.dy
		
		if(p.r<1) del(particles,p)
	end
end

function draw_ptc()
	for p in all(particles) do
		if(p.type=='r')circfill(p.x,p.y,p.r,p.col)
		if(p.type=='t')print(p.txt,p.x,p.y,7)
	end
end

