function _init()
	mode = "edit" 
	switch = true
	nb11 = 1
	xstart,ystart=64,64
	editmapcd = 0
	
	init_cursor()
	init_player()
	init_hotbar()
	
	particles = {}
	scrshake = 0
	
	camx,camy=0,0
end

function _update60()
	if mode == "edit" then
		update_cursor()
		editmap()
		
		-- exporting/importing
		menuitem(1,"‚¨ÜÔ∏è export level", function()
			make_textptc(2,120,"level copied to clipboard")
			printh(wd_str(),'@clip')
		end)
		
		menuitem(2,"‚¨áÔ∏è import level", function()
			wd_passt(stat(4))
			make_textptc(2,120,"level pasted from clipboard\n(ctrl+v if doesn't work)")
		end)
		
		if btnp(‚ùé) then 
			play_mode()
		end
		
		update_btns(hotbar)
		
	elseif mode == "play" then
		update_player()
		update_objs()
		if(btnp(‚ùé))edit_mode()
	end
	
	update_ptc()
	
	--screenshake
	scrshake=max(scrshake-.2,0)
	camera(camx+rrnd(scrshake),
	camy+rrnd(scrshake))
end

function _draw()
	cls()
	map()
	color(7)
	--print("debug:"..tostr(debug),0,16)
	if mode == "edit" then
		draw_btns(hotbar)
		--no code below this
		draw_cursor()
		
	elseif mode == "play" then
		spr(64,p.x,p.y)
		draw_objs()
		print("üÖæÔ∏è dash",2,2,7)
		
	end
	
	draw_ptc()
end

--functions

function abtn(b)
	return btn(b) or btn(b,1)
end

function sgn(n)
	if(n==nil)return nil
	if(n<0)return -1
	if(n>0)return 1
	return 0
end

function v(x,y)
	return {x=x,y=y}
end

function rrnd(n)
	return rnd(2*n)-n
end
