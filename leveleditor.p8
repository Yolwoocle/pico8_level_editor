function make_button(n,x,y,w,h,sp,txt,onclick)
	return {
		n=n,
		x=x, y=y,
		w=w, h=h,
		sp=sp,
		txt=txt,
		
		hovered=false,
		onclick=onclick,
	}
end

function update_btns(btns)
	for b in all(btns)do
		if point_rect_coll(mx,my,b.x,b.y,b.w,b.h) then
			b.hovered = true
			if lmb then
				--b:onclick()
			end
		end
		
		if my<30 then
			b.y+=(-9-b.y)/6
		else
			b.y+=(2-b.y)/6
		end
	end
end

function draw_btns(btns)
	for b in all(btns)do
		local x,y,w,h=b.x,b.y,b.w,b.h
		rectfill(x,y,x+w,y+h,1)
		spr(b.sp,x+1,y+1)
		if(b.n==cui)rect(x,y,x+w,y+h,7)
	end
	
	--remove for release
	if my>30 then
		print("‚ùé play  [scrl] item  [p] menu",2,btns[1].y+12,7)
	end
end

